# Copy this file to .env and fill in your own values
BASE_DIR="/absolute/path/to/video/files"
PEERTUBE_URL="https://your.peertube.instance"
PEERTUBE_USER="your_username"
PEERTUBE_PASS="your_password"
# Optional path for cached PeerTube video list used by match_peertube_videos.py
PEERTUBE_VIDEOS_JSON="peertube_videos.json"
# Set to true to have yt-dlp use cookies from Firefox
USE_FIREFOX_COOKIES="false"
# PostgreSQL connection info for set_publish_date.py
PGHOST="localhost"
PGPORT="5432"
PGDATABASE="peertube"
PGUSER="peertube"
PGPASSWORD="peertube_password"
